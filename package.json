        "gulp-less": "^5.0.0",
        "gulp-postcss": "^9.0.1",
        "gulp-rename": "^2.0.0",
        "gulp-sourcemaps": "^3.0.0",
        "jest": "^27.2.5",
        "lerna": "^4.0.0",
        "less": "4.1.2",
        "less-loader": "7.3.0",
        "lint-staged": "11.2.1",
        "mini-css-extract-plugin": "2.4.2",
