        "webpack-node-externals": "^3.0.0"
    },
    "peerDependencies": {
        "element-plus": "^1.1.0-beta.20",
        "js-cool": "^2.1.3",
        "vue": "^3.2.20"
    },
    "workspaces": [
        "packages/*",
        "examples"
    ],
    "gitHooks": {
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "packages/*.{js,jsx,vue}": [
            "vue-cli-service lint",
            "git add"
        ]
    },
    "author": {
