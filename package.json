{
    "name": "ui-lib-vue3",
    "description": "vue3.0组件库demo",
    "version": "0.1.0",
    "private": true,
    "main": "dist/index.cmd.js",
    "module": "dist/index.esm.js",
    "unpkg": "dist/index.cmd.js",
    "style": "dist/styles/index.css",
    "scripts": {
        "serve": "vue-cli-service serve",
        "lint": "vue-cli-service lint",
        "build-docs": "sh scripts/build-docs.sh",
        "docs:deploy": "GH=1 sh scripts/deploy-docs.sh",
        "create-module": "sh scripts/create-module.sh",
        "build-lib": "vue-cli-service build --target lib --name ui-lib-vue3 --modern --dest dist packages/index.js",
        "build-watch": "vue-cli-service build --target lib --name ui-lib-vue3 --modern --watch --dest dist packages/index.js",
        "build:utils": "cross-env BABEL_ENV=utils babel packages/utils --out-dir dist/utils -x .js,.ts --ignore *.test.js",
        "build:umd": "cross-env PUBLIC_PATH=/static/ui-lib-vue3/dist/ npx webpack --config build/webpack.umd.js",
        "build:common": "cross-env PUBLIC_PATH=/static/ui-lib-vue3/dist/ npx webpack --config build/webpack.common.js",
        "build:watch": "cross-env BUILD_WATCH=true npm run build:common",
        "build:server": "cross-env BUILD_WATCH=true PUBLIC_PATH=/static/ui-lib-vue3/dist/ npx webpack-dev-server --https --config build/webpack.umd.js",
        "build:file": "sh scripts/build-entry.sh && sh scripts/build-index.sh",
        "build:esm-bundle": "rollup --config build/rollup.config.bundle.js",
        "build:components": "rollup --config build/rollup.config.js",
        "build:component": "cross-env PUBLIC_PATH=/static/ui-lib-vue3/dist/ npx webpack --config build/webpack.component.js",
        "build:docs": "sh scripts/docs.sh",
        "build:css": "rimraf dist/styles && gulp",
        "build:watch-css": "gulp watch",
        "gen-version": "node scripts/gen-version",
        "prettier": "npx prettier --write ./",
        "eslint": "npx eslint packages --fix --ext .vue,.js",
        "dist": "run-s clean eslint prettier build:file docs build:common build:umd build:component",
        "docs": "rimraf doc && typedoc",
        "push": "run-s eslint prettier && sh scripts/push.sh",
        "clean": "rimraf dist",
        "analyz-lib": "cross-env ANALAYZ_REPORT=true npm run build-lib",
        "analyz:common": "cross-env ANALAYZ_REPORT=true npm run build:common",
        "analyz:umd": "cross-env ANALAYZ_REPORT=true npm run build:umd",
        "analyz:component": "cross-env ANALAYZ_REPORT=true npm run build:component",
        "report": "npx eslint-reporter ./module.json --output --format html --ext .vue,.js,.jsx,.ts"
    },
    "dependencies": {
        "core-js": "^3.18.2",
        "dayjs": "^1.10.7",
        "ui-lib-vue3": "workspace:*",
        "uuid": "^8.3.2"
    },
    "files": [
        "bin",
        "lib",
        "src"
    ],
    "devDependencies": {
        "@babel/cli": "7.15.7",
        "@babel/core": "7.15.8",
        "@babel/plugin-proposal-class-properties": "^7.14.5",
        "@babel/plugin-transform-runtime": "^7.15.8",
        "@babel/preset-env": "^7.15.8",
        "@babel/preset-typescript": "^7.15.0",
        "@babel/runtime": "^7.15.4",
        "@rollup/plugin-commonjs": "^21.0.0",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.0.5",
        "@rollup/plugin-typescript": "^8.2.5",
        "@saqqdy/prettier-config": "^1.0.4",
        "@types/jest": "^27.0.2",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "@vue/babel-plugin-jsx": "^1.1.0",
        "@vue/cli-plugin-babel": "4.5.13",
        "@vue/cli-plugin-eslint": "4.5.13",
        "@vue/cli-plugin-typescript": "^4.5.13",
        "@vue/cli-service": "4.5.13",
        "@vue/compiler-sfc": "^3.2.20",
        "@vue/eslint-config-prettier": "^6.0.0",
        "@vue/eslint-config-typescript": "^7.0.0",
        "@vuese/cli": "^2.14.3",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^27.2.5",
        "babel-loader": "^8.2.2",
        "babel-plugin-import": "^1.13.3",
        "babel-plugin-module-resolver": "^4.1.0",
        "colors": "^1.4.0",
        "cross-env": "^7.0.3",
        "css-loader": "^5.2.7",
        "cssnano": "4.1.11",
        "cssnano-preset-advanced": "4.0.8",
        "esbuild": "^0.13.4",
        "eslint": "^7.32.0",
        "eslint-config-sets": "^1.5.3",
        "eslint-plugin-prettier": "^3.4.1",
        "eslint-plugin-tsdoc": "^0.2.14",
